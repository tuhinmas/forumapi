{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": "off",
        "linebreak-style":"off",
        "camelcase":"off",
        "quotes": "off",
        "import/no-unresolved":"off",
        "import/extensions":"off",
        "import/no-extraneous-dependencies":"off",
        "no-underscore-dangle": "off",
        "class-methods-use-this":"off"
    }
}
